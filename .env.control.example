# ═══════════════════════════════════════════════
# Control Panel Configuration
# ═══════════════════════════════════════════════

# Telegram Bot Token (получить у @BotFather)
BOT_TOKEN=123456789:ABCdefGHIjklMNOpqrsTUVwxyz

# Telegram ID админов (через запятую, узнать у @userinfobot)
ADMIN_IDS=123456789,987654321

# ═══════════════════════════════════════════════
# Local Proxy (опционально - прокси на самом сервере Control Panel)
# ═══════════════════════════════════════════════

# Включить локальный прокси (true/false)
LOCAL_PROXY_ENABLED=true

# Порты локального прокси (чтобы не конфликтовали с нодами)
LOCAL_MTPROTO_PORT=8443
LOCAL_SOCKS5_PORT=1081

# Количество воркеров для локального MTProto
LOCAL_WORKERS=2

# MTProto секреты (оставьте пустым для автогенерации)
LOCAL_SECRET=

# Количество автогенерируемых секретов
LOCAL_SECRET_COUNT=1

# ═══════════════════════════════════════════════
# Optional: Web Interface
# ═══════════════════════════════════════════════

# WEB_PORT=3000
# WEB_ENABLED=false
