# control-panel
BOT_TOKEN=
ADMIN_IDS=

# Домен для автоматического HTTPS через Caddy (обязательно для продакшена)
# Если указан - Caddy автоматически получит SSL сертификат от Let's Encrypt
# Пример: DOMAIN=your-domain.com
# DOMAIN=

# Remnawave API (для входящих webhook'ов от Remnawave панели)
# Нужен только если Remnawave панель отправляет webhook'и на ваш сервер
# REMNAWAVE_API_KEY=change-me
REMNAWAVE_API_PORT=8081

# Backend API (опционально, для интеграции с веб-приложением)
# BACKEND_BASE_URL=http://127.0.0.1:3000
# BACKEND_TOKEN=

# Платежи (YooMoney) - опционально
# YOOMONEY_TOKEN=
# YOOMONEY_WALLET=

# Remnawave direct API (опционально, для прямого доступа к Remnawave API вместо backend)
# REMNAWAVE_BASE_URL=https://panel.example.com
# REMNAWAVE_TOKEN=

# Webhook URL для Remnawave (опционально, если используете webhook'и)
# Укажите URL вашего сервера, куда Remnawave будет отправлять webhook'и
# WEBHOOK_URL=https://your-domain.com/webhook
# WEBHOOK_SECRET_HEADER=your-secret-header-value

# Web API (для веб-приложения)
WEB_API_KEY=change-me
WEB_API_PORT=8082

# Redis (обязательно для продакшена с тысячами пользователей)
REDIS_HOST=redis
REDIS_PORT=6379

# node-agent (for docker-compose.node.yml)
API_TOKEN=change-me
DOMAIN=example.com
INTERNAL_IP=
MTPROTO_PORT=443
SOCKS5_PORT=1080
WORKERS=2
AD_TAG=
# MTProxy образ (рекомендуется skrashevich/MTProxy для статистики по секретам и TLS маскировки)
MT_PROXY_IMAGE=skrashevich/mtproxy:latest
# Альтернатива: MT_PROXY_IMAGE=telegrammessenger/proxy:latest
ENABLE_SOCKS5=false
MT_DOCKER_RUN_ARGS=
SOCKS5_DOCKER_RUN_ARGS=

# TLS маскировка (ОПЦИОНАЛЬНО - для обхода цензуры, маскировка под реальный сайт)
# Если не нужна TLS маскировка - оставьте эти переменные пустыми или закомментируйте
#
# Как получить сертификаты:
# 1. Через Let's Encrypt (бесплатно):
#    sudo apt-get install certbot
#    sudo certbot certonly --standalone -d your-domain.com
#    TLS_CERT_PATH=/etc/letsencrypt/live/your-domain.com/fullchain.pem
#    TLS_KEY_PATH=/etc/letsencrypt/live/your-domain.com/privkey.pem
#
# 2. Использовать существующий сертификат (если есть)
#
# 3. Пропустить TLS маскировку (оставить пустым) - РЕКОМЕНДУЕТСЯ для начала
#
# Пример: маскировка под web.max.ru
# TLS_DOMAIN=web.max.ru
# TLS_CERT_PATH=/etc/letsencrypt/live/web.max.ru/fullchain.pem
# TLS_KEY_PATH=/etc/letsencrypt/live/web.max.ru/privkey.pem
#
# Или оставить пустым (без TLS маскировки):
TLS_DOMAIN=
TLS_CERT_PATH=
TLS_KEY_PATH=


