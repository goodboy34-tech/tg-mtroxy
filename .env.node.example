# ═══════════════════════════════════════════════
# Node Agent Configuration
# ═══════════════════════════════════════════════

# API Token (генерируется автоматически при добавлении ноды в Control Panel)
API_TOKEN=your_generated_api_token_here

# Домен этого сервера
DOMAIN=proxy1.example.com

# Внутренний IP (для NAT, если сервер за NAT)
INTERNAL_IP=10.0.0.5

# Порты
MTPROTO_PORT=443
SOCKS5_PORT=1080
API_PORT=8080

# Количество воркеров MTProto (рекомендуется = количеству CPU ядер)
WORKERS=4

# MTProto секреты (генерируются через бота, можно указать несколько через запятую)
SECRET=

# Количество автогенерируемых секретов (если SECRET пустой)
SECRET_COUNT=1

# Рекламный тег MTProto (опционально, получить у @MTProxybot)
TAG=
