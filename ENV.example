# control-panel
BOT_TOKEN=
ADMIN_IDS=
REMNAWAVE_API_KEY=change-me
REMNAWAVE_API_PORT=8081
BACKEND_BASE_URL=http://127.0.0.1:3000
BACKEND_TOKEN=

# Платежи (YooMoney)
YOOMONEY_TOKEN=
YOOMONEY_WALLET=

# Remnawave direct API (опционально, если хотите использовать прямой доступ к Remnawave вместо backend)
REMNAWAVE_BASE_URL=https://panel.example.com
REMNAWAVE_TOKEN=

# Web API (для веб-приложения)
WEB_API_KEY=change-me
WEB_API_PORT=8082

# Redis (обязательно для продакшена с тысячами пользователей)
REDIS_HOST=redis
REDIS_PORT=6379

# node-agent (for docker-compose.node.yml)
API_TOKEN=change-me
DOMAIN=example.com
INTERNAL_IP=
MTPROTO_PORT=443
SOCKS5_PORT=1080
WORKERS=2
AD_TAG=
# MTProxy образ (рекомендуется skrashevich/MTProxy для статистики по секретам и TLS маскировки)
MT_PROXY_IMAGE=skrashevich/mtproxy:latest
# Альтернатива: MT_PROXY_IMAGE=telegrammessenger/proxy:latest
ENABLE_SOCKS5=false
MT_DOCKER_RUN_ARGS=
SOCKS5_DOCKER_RUN_ARGS=

# TLS маскировка (для обхода цензуры - маскировка под реальный сайт)
# Пример: маскировка под web.max.ru
# TLS_DOMAIN=web.max.ru
# TLS_CERT_PATH=/path/to/cert.pem
# TLS_KEY_PATH=/path/to/key.pem
# TLS_DOMAIN=
# TLS_CERT_PATH=
# TLS_KEY_PATH=


